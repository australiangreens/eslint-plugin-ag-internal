{
  "name": "@australiangreens/eslint-plugin-ag-internal",
  "version": "0.3.2",
  "private": false,
  "type": "module",
  "homepage": "https://github.com/australiangreens/eslint-plugin-ag-internal#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/australiangreens/eslint-plugin-ag-internal"
  },
  "description": "Internal linting configuration",
  "author": "The Australian Greens <webops@greens.org.au>",
  "contributors": [
    "Anthony Blond <anthony@unfinishedteleporter.com>"
  ],
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "lint": "eslint --max-warnings 0",
    "build": "tsc --build",
    "clean": "rimraf dist",
    "build:clean": "pnpm clean && pnpm build",
    "build:watch": "tsc --build --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@eslint/js": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-eslint-plugin": "^7.0.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^6.1.0",
    "eslint-plugin-react-refresh": "^0.4.23",
    "eslint-plugin-react-you-might-not-need-an-effect": "^0.5.5",
    "eslint-plugin-tsdoc": "^0.4.0",
    "typescript-eslint": "^8.45.0"
  },
  "devDependencies": {
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/node": "^24.6.2",
    "eslint": "^9.36.0",
    "globals": "^16.4.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "eslint": ">=9.26.0"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "packageManager": "pnpm@10.15.1",
  "files": [
    "dist",
    "!**/*.tsbuildinfo",
    "README.md",
    "LICENSE"
  ]
}